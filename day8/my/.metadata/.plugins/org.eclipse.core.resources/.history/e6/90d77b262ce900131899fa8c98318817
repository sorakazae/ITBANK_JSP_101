 <c:set var="dan" value="3"/>
 <c:forEach var="i" begin="1" end="10" step="1">
   <b>${"dan"}*${i}=${dan*i} </b><br>
 </c:forEach>
 

 
 <p>
<%
	int dan=3;
	for(int i=1; i<10; i++){
		out.println(dan+"*"+i+"="+(dan*i)+"&nbsp;&nbsp;");
	}
%>
	
